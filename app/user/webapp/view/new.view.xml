<mvc:View
    xmlns:form="sap.ui.layout.form"
    busyIndicatorDelay="0"
    height="100%"
    displayBlock="true"
    xmlns:c="sap.ui.core"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="app.user.controller.new"
>
    <Page
        id="_IDGenPage1"
        title="Vendor Information"
    >
        <content>
            <VBox id="_IDGenVBox1">
                <f:DynamicPage id="_IDGenDynamicPage1">
                    <f:content>
                        <VBox id="_IDGenVBox2">
                            <HBox id="_IDGenHBox1">
                                <Text
                                    id="_IDGenText1"
                                    text="The Shipping Corporation Of India"
                                />
                                <Text
                                    id="_IDGenText2"
                                    text="Address: 1st floor, Chandralok Building, 36 Janpath, Janpath Rd, Delhi, IN"
                                />
                                <Text
                                    id="_IDGenText3"
                                    text="Qualified: From 2022-04-01 To 2023-03-31"
                                />
                            </HBox>
                            <HBox id="_IDGenHBox2">
                                <f:GridContainer id="_IDGenGridContainer1">
                                    <f:GridContainerItem
                                        id="_IDGenGridContainerItem1"
                                    >
                                        <GenericTile
                                            id="_IDGenGenericTile1"
                                            header="Open Bids"
                                            frameType="TwoByOne"
                                            press="onOpenBidsPress"
                                        >
                                            <TileContent
                                                id="_IDGenTileContent1"
                                            >
                                                <NumericContent
                                                    id="_IDGenNumericContent1"
                                                    value="0"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                    </f:GridContainerItem>
                                    <f:GridContainerItem
                                        id="_IDGenGridContainerItem2"
                                    >
                                        <GenericTile
                                            id="_IDGenGenericTile2"
                                            header="Closed Bids"
                                            frameType="TwoByOne"
                                            press="onClosedBidsPress"
                                        >
                                            <TileContent
                                                id="_IDGenTileContent2"
                                            >
                                                <NumericContent
                                                    id="_IDGenNumericContent2"
                                                    value="0"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                    </f:GridContainerItem>
                                    <f:GridContainerItem
                                        id="_IDGenGridContainerItem3"
                                    >
                                        <GenericTile
                                            id="_IDGenGenericTile3"
                                            header="Ongoing Bids"
                                            frameType="TwoByOne"
                                            press="onOngoingBidsPress"
                                        >
                                            <TileContent
                                                id="_IDGenTileContent3"
                                            >
                                                <NumericContent
                                                    id="_IDGenNumericContent3"
                                                    value="1"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                    </f:GridContainerItem>
                                    <f:GridContainerItem
                                        id="_IDGenGridContainerItem4"
                                    >
                                        <GenericTile
                                            id="_IDGenGenericTile4"
                                            header="All Bids"
                                            frameType="TwoByOne"
                                            press="onAllBidsPress"
                                        >
                                            <TileContent
                                                id="_IDGenTileContent4"
                                            >
                                                <NumericContent
                                                    id="_IDGenNumericContent4"
                                                    value="1"
                                                />
                                            </TileContent>
                                        </GenericTile>
                                    </f:GridContainerItem>
                                </f:GridContainer>
                            </HBox>
                        </VBox>
                    </f:content>
                </f:DynamicPage>
                <Table
                    id="bidsTable"
                    visible="false"
                >
                    <columns>
                        <Column id="_IDGenColumn1">
                            <Text
                                id="_IDGenText4"
                                text="Bid ID"
                            />
                        </Column>
                        <Column id="_IDGenColumn2">
                            <Text
                                id="_IDGenText5"
                                text="Bid Start Date"
                            />
                        </Column>
                        <Column id="_IDGenColumn3">
                            <Text
                                id="_IDGenText6"
                                text="Bid Start Time"
                            />
                        </Column>
                        <Column id="_IDGenColumn4">
                            <Text
                                id="_IDGenText7"
                                text="Bid End Date"
                            />
                        </Column>
                        <Column id="_IDGenColumn5">
                            <Text
                                id="_IDGenText8"
                                text="Bid End Time"
                            />
                        </Column>
                        <Column id="_IDGenColumn6">
                            <Text
                                id="_IDGenText9"
                                text="Status"
                            />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem id="_IDGenColumnListItem1">
                            <cells>
                                <Text
                                    id="_IDGenText10"
                                    text="5000000024"
                                />
                                <Text
                                    id="_IDGenText11"
                                    text="2023-01-31"
                                />
                                <Text
                                    id="_IDGenText12"
                                    text="12:47:00"
                                />
                                <Text
                                    id="_IDGenText13"
                                    text="2023-02-03"
                                />
                                <Text
                                    id="_IDGenText14"
                                    text="12:57:00"
                                />
                                <Text
                                    id="_IDGenText15"
                                    text="Ongoing"
                                />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </VBox>
        </content>
    </Page>
</mvc:View>
